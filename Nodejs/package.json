{
  "name": "mangapresence",
  "version": "2.0.1",
  "description": "Server that talks with extension and discord-rpc library to update your discord presence based on what anime or manga you are currently watching",
  "main": "scripts/serverTray.js",
  "scripts": {
    "start": "node scripts/serverTray.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "systray": "^1.0.5"
  },
  "bin": {
    "my-command": "scripts/serverTray.js"
  },
  "pkg": {
    "assets": [
      "node_modules/systray/traybin/tray_windows_release.exe",
      "scripts/*.js",
      "icon/icon.ico",
      "icon/icon.png"
    ]
  }
}
